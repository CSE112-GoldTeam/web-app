<br>
<canvas id="myCanvas" width="1000" height="800" style="border:1px solid #d3d3d3;">

  </canvas>

  <script>
  var c = document.getElementById("myCanvas");
  var ctx = c.getContext("2d");
  ctx.font = "40px Georgia";
  ctx.fillText("Completed",40, 50);
  ctx.font = "20px Georgia";
  ctx.fillText("These forms are already complete with the information needed, simply click to edit or review.",40, 75);
  ctx.rect(40,120,250,250);
  ctx.rect(340,120,250,250);
  ctx.rect(640,120,250,250);
  ctx.fillText("First-Time Registration",60,390);
  ctx.fillText("Other Form",360,390);
  ctx.fillText("Forms",680,390);
  ctx.rect(40,440,250,250);
  ctx.rect(340,440,250,250);
  ctx.fillText("First-Time Registration",60,710);
  ctx.fillText("Other Form",360,710);
  ctx.stroke();
  </script>




