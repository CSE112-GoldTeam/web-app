<!DOCTYPE html>
<html>
<head>
    <title>{{ title }}</title>
    <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
    <!--<link rel='stylesheet' href='/stylesheets/style.css'/>-->
    <link rel='stylesheet' href='/stylesheets/landing.css'/>
    {{#newrelic}}
        {{{ newrelic.getBrowserTimingHeader }}}
    {{/newrelic}}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="navbar-header">
        <a class="navbar-brand" href="/">
            <img id = "company-logo" src="images/logo.png">
        </a>
      </div>
      <div class="navbar-padding-right">
        <ul class="nav navbar-nav navbar-right">
           <li>
               <p class="navbar-btn">
                   <a href="/login" class="standard-button" id="login-btn-navbar">Log In</a>
               </p>
           </li>
        </ul>
      </div>
    </nav>
    <!-- start of outer container -->
    <div class="container-fluid container-margin-landing landing-pic">
        <div class="row" id="first-section">
            <div id="intro" class="col-xs-7">
                <form class="form-group" action="" method="post">
                    <div class="col-xs-9">
                        <input type="company" class="small form-control" id="companyName" name="companyName" value="{{companyName}}" method="post" placeholder="Step 1: Enter company name">
                    </div>
                    <div class="col-xs-1 text-small">
                        <button type="submit" class="btn standard-button">Sign Up</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</body>
</html>
